{
  "ConnectionStrings": {
    "DefaultConnection": "Data source=./db/etosha-template.db"
  },
  "PasswordOptions": {
    "RequireDigit": false,
    "RequiredLength": 1,
    "RequiredUniqueChars": 1,
    "RequireLowercase": false,
    "RequireNonAlphanumeric": false,
    "RequireUppercase": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "JwtIssuerOptions": {
    "Key": "verySercretKeyThatNobodyKnows",
    "Issuer": "http://localhost:5880",
    "Audience": "http://localhost:5880"
  },
  "AllowedHosts": "*"
}
